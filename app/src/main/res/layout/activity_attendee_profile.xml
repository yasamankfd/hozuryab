<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Attendee_profile"
    android:background="@drawable/round_background">

    <Spinner
        android:id="@+id/attende_profile_bdate_day"
        android:layout_width="81dp"
        android:layout_height="48dp"
        app:layout_constraintBottom_toTopOf="@+id/attendee_editinfo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/attende_profile_bdate_year"
        app:layout_constraintTop_toBottomOf="@+id/attendee_day"
        tools:ignore="SpeakableTextPresentCheck"
        android:background="@drawable/round_textview"/>

    <TextView
        android:id="@+id/attendee_year"
        android:layout_width="70dp"
        android:layout_height="38dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/attende_profile_bdate_month"
        app:layout_constraintEnd_toStartOf="@+id/attendee_month"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attende_profile_phone" />

    <TextView
        android:id="@+id/attendee_month"
        android:layout_width="70dp"
        android:layout_height="38dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/attende_profile_bdate_year"
        app:layout_constraintEnd_toStartOf="@+id/attendee_day"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/attendee_year"
        app:layout_constraintTop_toBottomOf="@+id/attende_profile_phone" />

    <TextView
        android:id="@+id/attendee_day"
        android:layout_width="70dp"
        android:layout_height="38dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/attende_profile_bdate_day"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/attendee_month"
        app:layout_constraintTop_toBottomOf="@+id/attende_profile_phone" />

    <Spinner
        android:id="@+id/attende_profile_bdate_month"
        android:layout_width="81dp"
        android:layout_height="48dp"
        app:layout_constraintBottom_toTopOf="@+id/attendee_editinfo"
        app:layout_constraintEnd_toStartOf="@+id/attende_profile_bdate_year"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attendee_year"
        tools:ignore="SpeakableTextPresentCheck"
        android:background="@drawable/round_textview"/>

    <Spinner
        android:id="@+id/attende_profile_bdate_year"
        android:layout_width="81dp"
        android:layout_height="48dp"
        app:layout_constraintBottom_toTopOf="@+id/attendee_editinfo"
        app:layout_constraintEnd_toStartOf="@+id/attende_profile_bdate_day"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/attende_profile_bdate_month"
        app:layout_constraintTop_toBottomOf="@+id/attendee_month"
        tools:ignore="SpeakableTextPresentCheck"
        android:background="@drawable/round_textview"/>

    <Button
        android:id="@+id/uploadpic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/choose"
        app:layout_constraintBottom_toTopOf="@+id/attende_profile_uname"
        app:layout_constraintEnd_toStartOf="@+id/attende_profile_image"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/round_textview"/>

    <Button
        android:id="@+id/attendee_editinfo"
        android:layout_width="270dp"
        android:layout_height="50dp"
        android:paddingLeft="30dp"
        android:paddingRight="30dp"
        android:text="@string/edit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/round_textview"
        app:layout_constraintTop_toBottomOf="@+id/attende_profile_bdate_day" />

    <ImageView
        android:id="@+id/attende_profile_image"
        android:layout_width="136dp"
        android:layout_height="124dp"
        app:layout_constraintBottom_toTopOf="@+id/attende_profile_uname"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/uploadpic"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/round_textview"/>

    <EditText
        android:id="@+id/attende_profile_uname"
        android:layout_width="260dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:background="@drawable/round_textview"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/attende_profile_lname"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attende_profile_image"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/attende_profile_phone"
        android:layout_width="260dp"
        android:layout_height="50dp"
        android:background="@drawable/round_textview"
        android:ems="10"
        android:inputType="phone"
        android:minHeight="48dp"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/attendee_year"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attende_profile_name"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/attende_profile_lname"
        android:layout_width="260dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/attende_profile_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attende_profile_uname"
        tools:ignore="SpeakableTextPresentCheck"
        android:background="@drawable/round_textview"/>

    <EditText
        android:id="@+id/attende_profile_name"
        android:layout_width="260dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/attende_profile_phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attende_profile_lname"
        tools:ignore="SpeakableTextPresentCheck"
        android:background="@drawable/round_textview"/>


</androidx.constraintlayout.widget.ConstraintLayout>