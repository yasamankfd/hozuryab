<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/round_background"
    tools:context=".Attendee_profile">


    <EditText
        android:id="@+id/attende_profile_uname"
        android:layout_width="260dp"
        android:layout_height="50dp"
        android:layout_marginStart="75dp"
        android:layout_marginLeft="75dp"
        android:layout_marginTop="102dp"
        android:layout_marginEnd="75dp"
        android:layout_marginRight="75dp"
        android:layout_marginBottom="31dp"
        android:background="@drawable/round_textview"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/attende_profile_lname"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/attendee_month"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="423dp"
        android:layout_marginEnd="50dp"
        android:layout_marginRight="50dp"
        android:layout_marginBottom="30dp"
        android:gravity="center"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/attende_profile_bdate_month"
        app:layout_constraintEnd_toStartOf="@+id/attendee_day"
        app:layout_constraintStart_toEndOf="@+id/attendee_year"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/attende_profile_name"
        android:layout_width="260dp"
        android:layout_height="50dp"
        android:layout_marginStart="75dp"
        android:layout_marginLeft="75dp"
        android:layout_marginEnd="75dp"
        android:layout_marginRight="75dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/round_textview"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/attende_profile_phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attende_profile_lname"
        tools:ignore="SpeakableTextPresentCheck" />

    <Spinner
        android:id="@+id/attende_profile_bdate_yearr"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginStart="42dp"
        android:layout_marginLeft="42dp"
        android:layout_marginEnd="42dp"
        android:layout_marginRight="42dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/round_textview"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/attendee_editinfo"
        app:layout_constraintEnd_toStartOf="@+id/attende_profile_bdate_month"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attendee_year"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/attendee_editinfo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="70dp"
        android:layout_marginLeft="70dp"
        android:layout_marginEnd="70dp"
        android:layout_marginRight="70dp"
        android:layout_marginBottom="112dp"
        android:background="@drawable/round_textview"
        android:paddingLeft="30dp"
        android:paddingRight="30dp"
        android:text="@string/edit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attende_profile_bdate_month" />

    <TextView
        android:id="@+id/attendee_year"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="50dp"
        android:layout_marginEnd="51dp"
        android:layout_marginRight="51dp"
        android:layout_marginBottom="30dp"
        android:gravity="center"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/attende_profile_bdate_yearr"
        app:layout_constraintEnd_toStartOf="@+id/attendee_month"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attende_profile_phone" />

    <Spinner
        android:id="@+id/attende_profile_bdate_day"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginEnd="42dp"
        android:layout_marginRight="42dp"
        android:layout_marginBottom="192dp"
        android:background="@drawable/round_textview"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/attende_profile_bdate_month"
        app:layout_constraintTop_toBottomOf="@+id/attendee_day"
        tools:ignore="SpeakableTextPresentCheck" />

    <Spinner
        android:id="@+id/attende_profile_bdate_month"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginEnd="42dp"
        android:layout_marginRight="42dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/round_textview"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/attendee_editinfo"
        app:layout_constraintEnd_toStartOf="@+id/attende_profile_bdate_day"
        app:layout_constraintStart_toEndOf="@+id/attende_profile_bdate_yearr"
        app:layout_constraintTop_toBottomOf="@+id/attendee_month"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/attende_profile_lname"
        android:layout_width="260dp"
        android:layout_height="50dp"
        android:layout_marginStart="75dp"
        android:layout_marginLeft="75dp"
        android:layout_marginEnd="75dp"
        android:layout_marginRight="75dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/round_textview"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/attende_profile_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attende_profile_uname"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/attendee_day"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="423dp"
        android:layout_marginEnd="50dp"
        android:layout_marginRight="50dp"
        android:layout_marginBottom="30dp"
        android:gravity="center"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/attende_profile_bdate_day"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/attendee_month"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/attende_profile_phone"
        android:layout_width="260dp"
        android:layout_height="50dp"
        android:layout_marginStart="75dp"
        android:layout_marginLeft="75dp"
        android:layout_marginEnd="75dp"
        android:layout_marginRight="75dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/round_textview"
        android:ems="10"
        android:gravity="center"
        android:inputType="phone"
        android:minHeight="48dp"
        android:text=""
        app:layout_constraintBottom_toTopOf="@+id/attendee_year"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attende_profile_name"
        tools:ignore="SpeakableTextPresentCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>